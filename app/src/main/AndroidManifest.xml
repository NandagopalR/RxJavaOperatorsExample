<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nanda.rxjavaoperators">

  <application
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".ui.home.MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>

    <activity android:name=".ui.operators.RxJavaMathActivity"/>
    <activity android:name=".ui.rxmath.SumIntegerActivity"/>
    <activity android:name=".ui.rxmath.SumLongActivity"/>
    <activity android:name=".ui.rxmath.SumFloatActivity"/>
    <activity android:name=".ui.rxmath.SumDoubleActivity"/>
    <activity android:name=".ui.operators.ConcatExampleActivity"/>
    <activity android:name=".ui.operators.ZipExampleActivity"/>
    <activity android:name=".ui.operators.MergeActivity"/>
    <activity android:name=".ui.operators.FlatMapIterableActivity"/>

  </application>

</manifest>